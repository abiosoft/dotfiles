{
    "theme": "sync-with-os",
    "keymap": "vscode",
    "vimEmulationEnabled": true,
    "editor.fontSize": 14.0,
    "fus.sendAllowed": false,
    "editor.lineSpacing": 1.5,
    "terminal.lineSpacing": 1.5,
    "terminal.caretShape": "Block",
    "terminal.copyOnSelection": true,
    "editor.showInlayHints": true,
    "editor.caretBlinkingEnabled": false,
    "editor.formatOnSave": true,
    "editor.showTabs": true,
    "editor.indentStyle": "Spaces",
    "terminal.fontSize": 14.0,
    "editor.lineNumbers": "Relative",
    "[git-commit]": {},
    "files.exclude": [
        ".DS_Store",
        ".git"
    ]
}