{
  "window.autoDetectColorScheme": true,
  "window.customTitleBarVisibility": "never",
  "workbench.preferredDarkColorTheme": "Default Dark Modern",
  "workbench.preferredLightColorTheme": "Default Light Modern",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 15,
  "editor.fontWeight": "400",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontWeight": "400",
  "scm.inputFontFamily": "JetBrains Mono",
  "debug.console.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "debug.console.fontSize": 15,
  "editor.cursorBlinking": "solid",
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "editor.bracketPairColorization.enabled": false,
  "editor.inlayHints.enabled": "on",
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "*": {
        "bold": false,
      },
      "modifier": {
        "bold": true,
      },
      "keyword": {
        "bold": true,
      },
      "*.static": {
        "bold": true,
      },
    },
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true,
    "source.fixAll.eslint": true
  },
  "workbench.colorCustomizations": {
    "[Default Light Modern]": {
      "editor.background": "#f5f5f5",
      "activityBar.inactiveForeground": "#333333",
      "activityBar.foreground": "#111111",
      "activityBar.activeBorder": "#111111",
      "sideBar.background": "#f5f5f5",
      "terminal.background": "#f5f5f5",
    },
  },
  "go.useLanguageServer": true,
  "terminal.integrated.fontWeightBold": "normal",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.editor.showTabs": true,
  "go.addTags": {
    "tags": "json",
    "options": "json=omitempty",
    "promptForTags": true,
    "transform": "snakecase"
  },
  "debug.allowBreakpointsEverywhere": true,
  "yaml.completion": true,
  "yaml.hover": true,
  "[yaml]": {
    "editor.fontLigatures": true,
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "go.toolsManagement.checkForUpdates": "off",
  "codespaces.accountProvider": "GitHub",
  "go.delveConfig": {
    "dlvLoadConfig": {
      "followPointers": true,
      "maxVariableRecurse": 1,
      "maxStringLen": 1000,
      "maxArrayValues": 64,
      "maxStructFields": -1
    },
    "apiVersion": 2,
    "showGlobalVariables": false
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "window.nativeTabs": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "editor.lineNumbers": "relative",
  "[caddyfile]": {
    "editor.insertSpaces": true,
    "editor.formatOnSave": true
  },
  "telemetry.telemetryLevel": "off",
  "window.title": "${rootName}${separator}${activeEditorShort}",
  "vim.leader": ",",
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "c",
        "c"
      ],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "c",
        "c"
      ],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        {
          "command": "editor.action.showHover",
          "args": []
        }
      ]
    },
    {
      "before": [
        "g",
        "d"
      ],
      "commands": [
        {
          "command": "editor.action.goToDeclaration",
          "args": []
        }
      ]
    },
    {
      "before": [
        "g",
        "f"
      ],
      "commands": [
        {
          "command": "editor.action.navigateBack",
          "args": []
        }
      ]
    }
  ],
  "vim.useSystemClipboard": true,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.copyOnSelection": true,
  "csharp.referencesCodeLens.enabled": false,
  "docker.containers.label": "ContainerName",
  "editor.suggestSelection": "first",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "redhat.telemetry.enabled": false,
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml",
    "editor.formatOnSave": false
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "window.titleBarStyle": "custom",
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editorAssociations": {
    "*.md": "default"
  },
  "workbench.iconTheme": "vscode-icons",
  "svelte.enable-ts-plugin": true,
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[ruby]": {
    "editor.defaultFormatter": "rebornix.ruby",
    "editor.formatOnType": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.formatOnSave": true
  },
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.rulers": [
    120
  ],
  "editor.wordWrap": "off",
  "workbench.colorTheme": "Default Dark+ Experimental",
  "workbench.startupEditor": "none",
  "git.alwaysSignOff": true,
  "editor.renderWhitespace": "boundary",
  "svelte.plugin.svelte.compilerWarnings": {
    "a11y-click-events-have-key-events": "ignore",
    "a11y-aria-attributes": "ignore",
    "a11y-incorrect-aria-attribute-type": "ignore",
    "a11y-unknown-aria-attribute": "ignore",
    "a11y-hidden": "ignore",
    "a11y-misplaced-role": "ignore",
    "a11y-unknown-role": "ignore",
    "a11y-no-abstract-role": "ignore",
    "a11y-no-static-element-interactions": "ignore",
    "a11y-no-redundant-roles": "ignore",
    "a11y-role-has-required-aria-props": "ignore",
    "a11y-accesskey": "ignore",
    "a11y-autofocus": "ignore",
    "a11y-misplaced-scope": "ignore",
    "a11y-positive-tabindex": "ignore",
    "a11y-invalid-attribute": "ignore",
    "a11y-missing-attribute": "ignore",
    "a11y-img-redundant-alt": "ignore",
    "a11y-label-has-associated-control": "ignore",
    "a11y-media-has-caption": "ignore",
    "a11y-distracting-elements": "ignore",
    "a11y-structure": "ignore",
    "a11y-mouse-events-have-key-events": "ignore",
    "a11y-missing-content": "ignore"
  }
}
